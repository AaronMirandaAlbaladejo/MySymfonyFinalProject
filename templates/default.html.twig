<!-- templates/default.html.twig -->
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en">
  <head>
    <title>Web site</title>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="title" content="Web site" />
    <meta name="description" content="Site description here" />
    <meta name="keywords" content="keywords here" />
    <meta name="language" content="en" />
    <meta name="subject" content="Site subject here" />
    <meta name="robots" content="All" />
    <meta name="copyright" content="Your company" />
    <meta name="abstract" content="Site description here" />
    <meta name="MSSmartTagsPreventParsing" content="true" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
    <script defer src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js" integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN" crossorigin="anonymous"></script>
    <!-- <link id="theme" rel="stylesheet" type="text/css" href="style.css" title="theme" />-->
	<link href="{{ asset('/css/style.css') }}" rel="stylesheet" type="text/css" />
  </head>
  <body> 
    <div id="wrapper"> 
      <div id="bg"> 
        <div id="header"></div>  
        <div id="page"> 
          <div id="container"> 
            <!-- banner -->  
            <div id="banner"></div>  
            <!-- end banner -->  
            <!-- horizontal navigation -->  
			
			
            <div id="nav1"> 
            {% block menu %}
				{{ render(controller(  'App\\Controller\\MenuController:menu')) }}
			
			{% endblock %}
            </div>  
      
			
			<!-- end horizontal navigation -->  
            <!--  content -->  
            <div id="content"> 
              <!--  main --> 
			  <div id="center"> 
               
                {% block contenido %}

				{% endblock %} 
				
              </div>  

              <!-- right--> 
			  <div id="right"> 
			  {% block right %} {% endblock %} 
			  
                
              </div>  
             
            </div>  
            <!-- end content --> 
          </div>  
          <!-- end container --> 
        </div>  
        <div id="footerWrapper"> 
          <div id="footer"> 
            {% block footer %}
            {{ render(controller('App\\Controller\\FooterController::footer')) }}
            {% endblock %}
            <p> 
              <!-- 
DO NOT REMOVE OR MODIFY THE FOOTER LINK BELOW.
If you want to remove this link please make a 10 dollars donation at www.dotemplate.com
--> Web template created with 
              <a href="http://www.dotemplate.com" shape="rect">doTemplate</a> free templates design tool.
            </p> 
          </div> 
        </div> 
      </div> 
    </div> 
  </body>
</html>
